<Window x:Class="main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:main"
        mc:Ignorable="d"
        Title="{DynamicResource rTitle}" Height="450" Width="800" Style="{DynamicResource WindowStyle}"> 
    <Grid>
        <TabControl x:Name="MainMenu" Style="{DynamicResource TabControlStyle}">
            <TabItem Header= "{DynamicResource rTitle}" Style="{DynamicResource TabItemStyle}">
                <Frame Source="Resourses/TabMAIN.xaml"/>
            </TabItem>
            <TabItem Header= "{DynamicResource rTabSetting}" >
                <Frame Source="Resourses/TabSETTING.xaml"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
   